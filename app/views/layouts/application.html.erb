<!DOCTYPE html>
<html>
  <head>
    <title>FAO Database</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar fixed-top navbar-light bg-light">
      <a class="navbar-brand" href="#">
        <%= image_tag('FAO_logo.svg', width: '30', heigth: '30', class: 'd-inline-block align-top') %>
        <!-- <img src="FAO_logo.svg" width="30" height="30" class="d-inline-block align-top" alt=""> -->
        FAO questionnaires database
      </a>
    </nav>
    <%= yield %>
  </body>
</html>
